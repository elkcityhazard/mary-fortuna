nslateX(-100%);-moz-transform:translateX(-100%)}.swiper-button-next{transform:translateX(100%);-webkit-transform:translateX(100%);-moz-transform:translateX(100%)}.swiper{min-width:300px;max-width:360px}.swiper:hover .swiper-button-prev,.swiper:hover .swiper-button-next{opacity:1;transform:translateX(0%);-webkit-transform:translateX(0%);-moz-transform:translateX(0%)}@media screen and (min-width:1200px){.swiper{max-width:600px}}.card{display:grid;gap:1rem!important;max-width:450px!important;min-width:300px!important;min-height:450px;place-items:center;margin:1.5rem auto!important}.card .inner-wrapper{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;border-radius:4px;background:rgba(0,0,0,.75);color:#fff;display:grid;height:100%;place-items:center;max-width:100%!important;overflow:hidden;width:100%}.card .inner-wrapper:hover{background:0 0;border:3px solid orange}.card .card-button{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;border-color:#fff!important;color:#fff!important;overflow:hidden;text-decoration:none!important;word-wrap:wrap;max-width:100%}.card .card-button:hover{border-color:orange!important;outline-color:orange!important}.grid-container{display:grid;gap:0 1rem;grid-template-columns:repeat(auto-fill,minmax(275px,1fr));grid-auto-flow:row dense;grid-auto-rows:10px}.grid-container .grid-item{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;border-radius:10px;overflow:hidden}.grid-container .grid-item a span{padding:1rem;transition:all 500ms ease;-webkit-transition:all 500ms ease}.grid-container .grid-item:hover a span{opacity:0}.waves-container{background:0 0;position:relative}.waves-container .waves{bottom:.9rem;display:none;left:0;right:0;position:absolute;z-index:-1}