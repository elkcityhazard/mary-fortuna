h1{font-size:3.5rem}h2{font-size:2.75rem}h3{font-size:1.5rem}a{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear}p{margin:1rem auto}img{height:auto;max-width:100%}img.lazy{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;opacity:0}ul{margin:1rem auto;max-width:60ch}.social-icon{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;align-items:center;border-radius:50%;display:flex;height:50px;justify-content:center;width:50px}.social-icon .fa{transition:all 250ms linear;-webkit-transition:all 250ms linear;-moz-transition:all 250ms linear;color:#fff}.social-icon .fa:hover{color:#666}.social-icon:hover{background-color:#e6e6e6!important}.social-icon:hover .fa{color:#333}header p{margin:0}.section{margin:auto}.section h1,.section h2,.section h3,.section h4,.section h5,.section h6{text-align:center}#mainNavBar{transition:all 500ms ease;-webkit-transition:all 500ms ease;-moz-transition:all 500ms ease;opacity:0}@media screen and (min-width:960px){#mainNavBar{opacity:1}}.swiper-lazy{max-height:75vh;object-fit:contain}.swiper-button-prev,.swiper-button-next{color:#000;opacity:0;transition:all 500ms ease;-webkit-transition:all 500ms ease;-moz-transition:all 500ms ease}.swiper-button-prev{transform:translateX(-100%);-webkit-transform:translateX(-100%);-moz-transform:translateX(-100%)}.swiper-button-next{transform:translateX(100%);-webkit-transform:translateX(100%);-moz-transform:translateX(100%)}.swiper{min-width:300px;max-width:360px}.swiper:hover .swiper-button-prev,.swiper:hover .swiper-button-next{opacity:1;transform:translateX(0%);-webkit-transform:translateX(0%);-moz-transform:translateX(0%)}@media screen and (min-width:1200px){.swiper{max-width:600px}}.list-card .taxonomy-button{background:0 0;border-radius:1.5rem;-webkit-border-radius:1.5rem;-moz-border-radus:1.5rem}.list-card .taxonomy-button:hover{border-color:orange}.grid-container{display:grid;grid-auto-rows:10px;grid-auto-flow:dense;height:100%;width:100%}